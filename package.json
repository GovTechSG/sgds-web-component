{
  "name": "@govtechsg/sgds-web-component",
  "version": "0.0.12",
  "description": "",
  "main": "lib/umd/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:dev": "rm -rf lib && rollup --config rollup.config.js --environment NODE_ENV:development",
    "build:prod": "rm -rf lib && rollup --config rollup.config.js --environment NODE_ENV:production",
    "build:react": "npm run cem && node ./scripts/makeReact.mjs",
    "build:readme": "gulp build-readme",
    "write:stories": "npm run cem && node ./scripts/generateStories.mjs && npm run concat:stories",
    "write:component": "plop",
    "build:lib": "npm run build:react && npm run build:prod && npm run post:build",
    "build:watch": "npm run build:dev -- --watch",
    "post:build": "node ./scripts/frankBuild.js",
    "dev": "npm run build:watch & npm run serve",
    "serve": "wds --watch --open",
    "test": "rm -rf test-outdir && rollup --config rollup.test.config.js && web-test-runner",
    "test:scripts": "wtr scripts/__tests__/**/*.test.js --node-resolve",
    "storybook": "npm run build:dev && nodemon --exec \"npm run write:stories &&  STORYBOOK_ENV=development start-storybook -p 6006\" --watch stories/templates stories/getting-started -e js,mdx",
    "build-storybook": "npm run build:dev && npm run write:stories && STORYBOOK_ENV=production build-storybook",
    "prepare": "husky install",
    "concat:stories": "gulp concat-storybook-mdx",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "cem": "cem analyze --litelement "
  },
  "author": "GovTechSG",
  "contributors": [
    {
      "name": "rav3",
      "url": "https://github.com/rav3"
    },
    {
      "name": "haphizi",
      "url": "https://github.com/haphizi"
    },
    {
      "name": "clukhei",
      "url": "https://github.com/clukhei"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@govtechsg/sgds": "^2.1.1",
    "@lit-labs/react": "^1.0.9",
    "bootstrap": "^5.1.3",
    "lit": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/storybook": "^0.2.8",
    "@open-wc/testing": "^3.0.0-next.5",
    "@playwright/test": "^1.27.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^5.0.0",
    "@shoelace-style/shoelace": "^2.0.0-beta.83",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/web-components": "^6.5.16",
    "@types/bootstrap": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@web/dev-server": "^0.1.17",
    "@web/dev-server-esbuild": "^0.3.2",
    "@web/dev-server-rollup": "^0.3.19",
    "@web/dev-server-storybook": "^0.5.4",
    "@web/test-runner": "^0.14.0",
    "@web/test-runner-commands": "^0.6.5",
    "@web/test-runner-playwright": "^0.8.10",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cem-plugin-better-lit-types": "^0.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "esbuild-sass-plugin": "^2.3.2",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-wc": "^1.4.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
