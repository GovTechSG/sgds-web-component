{
  "name": "@govtechsg/sgds-web-component",
  "version": "3.11.0",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GovTechSG/sgds-web-component.git"
  },
  "scripts": {
    "dev": "VITE_ENV=development vite --open",
    "preview": "VITE_ENV=production vite --open",
    "build": "npm run cem && npm run build:react && rm -rf lib && rollup --config rollup.config.mjs && node ./scripts/frankBuild.js && npm run generate:icons && npm run build:fouc",
    "build:react": "node ./scripts/makeReact.mjs",
    "build:readme": "gulp build-readme",
    "build:fouc": "node ./scripts/generateFoucStyle.mjs",
    "write:stories": "npm run cem && node ./scripts/generateStories.mjs && npm run concat:stories && npm run update:docs",
    "write:component": "plop",
    "test": "rm -rf test-outdir && rollup --config rollup.test.config.js && web-test-runner",
    "test:scripts": "wtr scripts/**/__tests__/**/*.test.{js,ts} --node-resolve",
    "prepare": "husky install",
    "concat:stories": "gulp concat-stories-js && gulp concat-storybook-mdx",
    "update:docs": "gulp replace-version-and-sri",
    "format": "prettier --ignore-path .prettierignore  --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .prettierignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "cem": "cem analyze --litelement ",
    "e2e-test": "node cypress/scripts/run_all.js & cypress open",
    "storybook": "nodemon --exec \"npm run utility:storybook && npm run write:stories &&  STORYBOOK_ENV=development storybook dev -p 6006\" --watch stories/templates stories/getting-started src/css/utility.css -e js,mdx,css",
    "build-storybook": "npm run utility:storybook && npm run build && npm run write:stories && STORYBOOK_ENV=production storybook build --loglevel verbose",
    "convert:icons": "node scripts/convertIcons.js && pnpm run generate:icons",
    "generate:icons": "node scripts/generateIconRegistry.js",
    "sync-figma-to-tokens": "tsx scripts/figma/sync-figma-to-tokens/sync-figma-to-tokens.ts",
    "chromatic": "chromatic",
    "utility:dev": "tailwindcss -i ./src/css/utility.css -o ./playground/css/utility.css --config tailwind.config.js --watch",
    "utility:storybook": "tailwindcss -i ./src/css/utility.css -o ./.storybook/utility.css --config tailwind.config.js"
  },
  "author": "GovTechSG",
  "contributors": [
