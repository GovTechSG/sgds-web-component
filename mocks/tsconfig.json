  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es2017", "dom", "dom.iterable"],
    "sourceMap": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "rootDir": ".",
    "allowJs": true
  }
}
